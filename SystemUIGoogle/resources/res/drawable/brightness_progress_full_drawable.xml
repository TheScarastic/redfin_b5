    </item>
    <item android:gravity="right|center_vertical|center_horizontal|center" android:id="@+id/slider_icon" android:height="@dimen/rounded_slider_icon_size" android:width="@dimen/rounded_slider_icon_size" android:right="@dimen/rounded_slider_icon_inset">
        <com.android.systemui.util.AlphaTintDrawableWrapper android:tint="?android:attr/textColorPrimaryInverse" android:drawable="@drawable/ic_brightness"/>
    </item>
</layer-list>
