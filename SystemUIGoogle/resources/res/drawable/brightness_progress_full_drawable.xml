<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:priv-android="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" android:autoMirrored="true">
    <item android:id="@+id/slider_foreground" android:height="@dimen/rounded_slider_height">
        <shape>
            <size android:height="@dimen/rounded_slider_height"/>
            <solid android:color="?android:^attr-private/colorListDivider"/>
            <corners android:radius="@dimen/rounded_slider_corner_radius"/>
