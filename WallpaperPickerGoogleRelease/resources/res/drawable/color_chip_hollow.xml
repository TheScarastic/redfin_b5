<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center" android:id="@+id/center_fill" android:height="@dimen/component_color_chip_size" android:width="@dimen/component_color_chip_size">
        <shape android:shape="oval">
            <solid android:color="@android:color/black"/>
        </shape>
    </item>
    <item android:gravity="center" android:height="@dimen/component_color_chip_container_size" android:width="@dimen/component_color_chip_container_size">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="24dp" android:thickness="@dimen/option_border_width">
            <solid android:color="?android:attr/colorAccent"/>
        </shape>
    </item>
</layer-list>
